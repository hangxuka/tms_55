<% provide :title, t(".all_subject") %>
<div class = "page-header">
  <h2><%= t ".all_subject" %></h2>
  <p><%= link_to t(".new"), new_supervisor_user_path %></p>
</div>
<hr>
<table class = "table table-striped">
  <thead>
    <tr>
      <th><%= t "name" %></th>
      <th><%= t "start_date" %></th>
      <th><%= t "end_date"%></th>
      <th><%= t "edit" %></th>
      <th><%= t "delete" %></th>
    </tr>
  </thead>
  <tbody>
    <% @subjects.each do |subject| %>
      <tr>
        <td><%= subject.name %></td>
        <td><%= subject.start_date %></td>
        <td><%= subject.start_date %></td>
        <td><%= link_to t("edit"), "#" %></td>
        <td><%= link_to t("delete"), "#" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
