<h1><%= t ".update" %></h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @user_subject do |f| %>

      <%= f.label :status %>
      <%= f.text_field :status, class: "form-control" %>

      <%= f.submit t(".savechange"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
